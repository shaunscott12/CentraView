<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ejb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD Enterprise JavaBeans 2.0//EN" "http://java.sun.com/dtd/ejb-jar_2_0.dtd">
<ejb-jar>
	<enterprise-beans>
		<session>
			<ejb-name>RulesManage</ejb-name>
			<home>com.centraview.email.rules.RulesManageHome</home>
			<remote>com.centraview.email.rules.RulesManage</remote>
			<local-home>com.centraview.email.rules.RulesManageLocalHome</local-home>
			<local>com.centraview.email.rules.RulesManageLocal</local>
			<ejb-class>com.centraview.email.rules.RulesManageEJB</ejb-class>
			<session-type>Stateless</session-type>
			<transaction-type>Bean</transaction-type>
		</session>
	</enterprise-beans>
