<?xml version="1.0"?>
<!DOCTYPE ejb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD Enterprise JavaBeans 2.0//EN" "http://java.sun.com/dtd/ejb-jar_2_0.dtd">

<ejb-jar>
	<enterprise-beans>
		<session>
			<ejb-name>EmailSettings</ejb-name>
			<home>com.centraview.administration.emailsettings.EmailSettingsHome</home>
			<remote>com.centraview.administration.emailsettings.EmailSettings</remote>
			<ejb-class>com.centraview.administration.emailsettings.EmailSettingsEJB</ejb-class>
			<local-home>com.centraview.administration.emailsettings.EmailSettingsLocalHome</local-home>
			<local>com.centraview.administration.emailsettings.EmailSettingsLocal</local>
			<session-type>Stateless</session-type>
			<transaction-type>Bean</transaction-type>
		</session>
	</enterprise-beans>
</ejb-jar>