<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE ejb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD Enterprise JavaBeans 2.0//EN" "http://java.sun.com/dtd/ejb-jar_2_0.dtd">

<ejb-jar>
 <enterprise-beans>
  <entity>
   <ejb-name>HrExpense</ejb-name>
   
    <home>com.centraview.hr.hrexpenses.HrExpensesHome</home>
   <remote>com.centraview.hr.hrexpenses.HrExpenses</remote>

   <local-home>com.centraview.hr.hrexpenses.HrExpensesLocalHome</local-home>
   <local>com.centraview.hr.hrexpenses.HrExpensesLocal</local>

   <ejb-class>com.centraview.hr.hrexpenses.HrExpensesEJB</ejb-class>
   
   <prim-key-class>com.centraview.hr.hrexpenses.ExpenseFormPK</prim-key-class>
   
   <persistence-type>Bean</persistence-type>
   <transaction-type>Bean</transaction-type>
   <reentrant>False</reentrant>
   
  </entity>
 </enterprise-beans>

</ejb-jar>
