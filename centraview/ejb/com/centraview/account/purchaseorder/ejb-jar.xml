<ejb-jar>
 <enterprise-beans>
  <entity>
   <ejb-name>PurchaseOrder</ejb-name>
   
    <home>com.centraview.account.purchaseorder.PurchaseOrderHome</home>
   <remote>com.centraview.account.purchaseorder.PurchaseOrder</remote>

   <local-home>com.centraview.account.purchaseorder.PurchaseOrderLocalHome</local-home>
   <local>com.centraview.account.purchaseorder.PurchaseOrderLocal</local>

   <ejb-class>com.centraview.account.purchaseorder.PurchaseOrderEJB</ejb-class>
   
   <prim-key-class>com.centraview.account.purchaseorder.PurchaseOrderPK</prim-key-class>
   
   <persistence-type>Bean</persistence-type>
   <transaction-type>Bean</transaction-type>
   <reentrant>False</reentrant>
   
  </entity>
 </enterprise-beans>

</ejb-jar>
