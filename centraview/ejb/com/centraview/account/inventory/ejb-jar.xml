<ejb-jar>
  <enterprise-beans>
    <session>
      <ejb-name>Inventory</ejb-name>
      <home>com.centraview.account.inventory.InventoryHome</home>
      <remote>com.centraview.account.inventory.Inventory</remote>
      <ejb-class>com.centraview.account.inventory.InventoryEJB</ejb-class>
      <local-home>com.centraview.account.inventory.InventoryLocalHome</local-home>
      <local>com.centraview.account.inventory.InventoryLocal</local>
      <session-type>Stateless</session-type>
      <transaction-type>Bean</transaction-type>
    </session>
  </enterprise-beans>
</ejb-jar>
