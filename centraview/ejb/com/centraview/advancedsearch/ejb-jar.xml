<ejb-jar>
  <enterprise-beans>
    <session>
	  <ejb-name>AdvancedSearch</ejb-name>
	  <home>com.centraview.advancedsearch.AdvancedSearchHome</home>
	  <remote>com.centraview.advancedsearch.AdvancedSearch</remote>
	  <local-home>com.centraview.advancedsearch.AdvancedSearchLocalHome</local-home>
