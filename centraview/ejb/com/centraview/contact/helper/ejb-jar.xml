<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE ejb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD Enterprise JavaBeans 2.0//EN" "http://java.sun.com/dtd/ejb-jar_2_0.dtd">

<ejb-jar>
 <enterprise-beans>
  <session>
   <ejb-name>ContactHelper</ejb-name>
   
   <home>com.centraview.contact.helper.ContactHelperHome</home>
   <remote>com.centraview.contact.helper.ContactHelper</remote>
   
   <local-home>com.centraview.contact.helper.ContactHelperLocalHome</local-home>
   <local>com.centraview.contact.helper.ContactHelperLocal</local>
   
   <ejb-class>com.centraview.contact.helper.ContactHelperEJB</ejb-class>
   
   <session-type>Stateless</session-type>
   <transaction-type>Bean</transaction-type>
  </session>
 </enterprise-beans>

