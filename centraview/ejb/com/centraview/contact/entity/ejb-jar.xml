<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE ejb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD Enterprise JavaBeans 2.0//EN" "http://java.sun.com/dtd/ejb-jar_2_0.dtd">

<ejb-jar>
 <enterprise-beans>
  <entity>
   <ejb-name>Entity</ejb-name>
   
   <home>com.centraview.contact.entity.EntityHome</home>
   <remote>com.centraview.contact.entity.Entity</remote>
   
   <local-home>com.centraview.contact.entity.EntityLocalHome</local-home>
   <local>com.centraview.contact.entity.EntityLocal</local>

   <ejb-class>com.centraview.contact.entity.EntityEJB</ejb-class>
   
   <prim-key-class>com.centraview.contact.entity.EntityPK</prim-key-class>
   
   <persistence-type>Bean</persistence-type>
   <transaction-type>Bean</transaction-type>
   <reentrant>False</reentrant>
   
  </entity>
 </enterprise-beans>

 
