<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ejb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD Enterprise JavaBeans 2.0//EN" "http://java.sun.com/dtd/ejb-jar_2_0.dtd">
<ejb-jar>
  <enterprise-beans>
    <entity>
      <ejb-name>Individual</ejb-name>
      <home>com.centraview.contact.individual.IndividualHome</home>
      <remote>com.centraview.contact.individual.Individual</remote>
      <ejb-class>com.centraview.contact.individual.IndividualEJB</ejb-class>
      <prim-key-class>com.centraview.contact.individual.IndividualPK</prim-key-class>
      <persistence-type>Bean</persistence-type>
      <transaction-type>Bean</transaction-type>
      <reentrant>False</reentrant>
      <local-home>com.centraview.contact.individual.IndividualLocalHome</local-home>
      <local>com.centraview.contact.individual.IndividualLocal</local>
    </entity>
  </enterprise-beans>
</ejb-jar>
