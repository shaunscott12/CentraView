<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE ejb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD Enterprise JavaBeans 2.0//EN" "http://java.sun.com/dtd/ejb-jar_2_0.dtd">

<ejb-jar>
 <enterprise-beans>
  <session>
   <ejb-name>Group</ejb-name>

    <home>com.centraview.contact.group.GroupHome</home>
   <remote>com.centraview.contact.group.Group</remote>

   <local-home>com.centraview.contact.group.GroupLocalHome</local-home>
   <local>com.centraview.contact.group.GroupLocal</local>
   
   <ejb-class>com.centraview.contact.group.GroupEJB</ejb-class>
   
   
   <session-type>Stateless</session-type>
   <transaction-type>Bean</transaction-type>
   
  </session>
 </enterprise-beans>
</ejb-jar>
