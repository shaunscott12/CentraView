  <data-sources/> 
  <form-beans/>
  <global-exceptions/>
  <global-forwards/>
  
  <action-mappings>
    <action path="/additional_menu" type="com.centraview.common.AdditionalMenuHandler">
      <forward name="help" path="/help/centraview.htm" redirect="true" />
      <forward name="preferences" path="/preference/user_profile.do"/>
